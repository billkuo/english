function play()
{
	var removeembed=document.getElementsByTagName("embed")[0];
	document.body.removeChild(removeembed);
	
	var embed=document.createElement("embed");
	embed.src="/temp/Case120160206171925.mp3";
	embed.loop="-1";
	embed.hidden="true";
	embed.style="height: 0px; width: 0px";
	embed.type="audio/mpeg";
	document.body.appendChild(embed);
}
